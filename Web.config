<?xml version="1.0"?>
<configuration>
  
  <connectionStrings>
    <!--<add name="CS" connectionString="Data Source=(LocalDB)\v11.0; AttachDbFileName=|DataDirectory|\OES.mdf; Integrated Security=true;"></add>-->
    <add name="CS" connectionString="Data Source=(local)\; Initial Catalog=MCES; Integrated Security=true;"/>
  </connectionStrings>

  <system.web>
    <authentication mode="Forms" >
      <forms  name="cookie99" loginUrl="~/index.aspx" timeout="39" defaultUrl="~/Student/ques.aspx"  >
      </forms>
    </authentication>

    <authorization>
      <allow users="*"/>
    </authorization>

    <compilation debug="true" targetFramework="4.0"></compilation>

  </system.web>

  <location path="Student">
    <system.web>
      <authorization>
        <deny users="?" />
      </authorization>
    </system.web>
  </location>

  <location path="Admin">
    <system.web>
      <authorization>
        <allow users="admin"/>
        <deny users="*" />
      </authorization>
    </system.web>
  </location>

</configuration>